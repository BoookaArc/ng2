{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types",
      "src/core/types"
    ],
    "paths": {
      "ng2-qgrid/theme/material": ["projects/ngx-qgrid-theme-material/public-api.ts"],
      "ng2-qgrid/theme/basic": ["projects/ngx-qgrid-theme-basic/public-api.ts"],
      "ng2-qgrid": ["projects/ng2-qgrid/src/public-api.ts"],
      "qgrid/core/*": ["projects/qgrid-core/src/*"],
      "qgrid/plugins/*": ["projects/qgrid-plugins/src/*"],
      "ngx-qgrid": ["projects/ngx-qgrid/public-api.ts"],
      "ngx-qgrid/plugins": ["projects/ngx-qgrid-plugins/public-api.ts"]
    },
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "enableIvy": false
  }
}