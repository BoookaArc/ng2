{
  "name": "ng2-qgrid",
  "version": "7.2.5",
  "scripts": {
    "ng": "ng",
    "start": "node build.serve",
    "start:prod": "node build.serve.prod",
    "clean": "rimraf out-tsc dist",
    "copy:npm": "node build.copy.js **/* dist node_modules/ng2-qgrid",
    "lint": "ng lint",
    "test": "ng test",
    "test:debug": "ng test --browsers ChromeDebug",
    "test:screenshot": "protractor ./e2e/protractor.conf.js",
    "test:screenshot-update-goldens": "protractor ./e2e/protractor-update-goldens.conf.js",
    "pretest:screenshot": "if not exist e2e\\diff mkdir e2e\\diff && webdriver-manager update --gecko=false",
    "build:app": "ng build --prod --output-path dist/app --base-href /ng2/",
    "build:lib": "npm run clean && node build.lib.js",
    "build:release": "npm run build:lib && npm run copy:npm && node build.theme.js",
    "build:theme": "npm run clean && node build.theme.js",
    "release:example": "node ./build.example",
    "release:lib": "npm run build:release && cd dist && npm publish",
    "release:app": "npm run build:app && npx angular-cli-ghpages --dir=dist/app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "core-js": "^2.6.9",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cdk": "~7.3.7",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angular/material": "~7.3.7",
    "@angular/router": "~7.2.15",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cucumber": "^4.0.5",
    "@types/lodash-es": "^4.17.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "~12.0.8",
    "angular-cli-ghpages": "^0.5.3",
    "blue-harvest": "^0.3.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "codelyzer": "~5.1.0",
    "command-line-args": "^5.1.1",
    "css.escape": "^1.5.1",
    "cucumber": "^5.1.0",
    "fastdom": "^1.0.9",
    "file-saver": "^2.0.2",
    "karma": "~4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "livr": "^2.2.5",
    "lodash-es": "^4.17.11",
    "mocha": "^6.1.4",
    "ng-packagr": "^5.3.0",
    "npm-sass": "^2.3.0",
    "protractor": "~5.4.2",
    "protractor-cucumber-framework": "^6.1.2",
    "rollup": "^1.15.6",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.1.0-1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.3",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-uglify": "^6.0.2",
    "sane": "^4.1.0",
    "shelljs": "^0.8.3",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.17.0",
    "typescript": "~3.2.2",
    "xlsx": "^0.14.3",
    "ng2-qgrid": "*"
  }
}